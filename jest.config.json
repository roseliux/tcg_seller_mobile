{
  "preset": "react-native",
  "setupFilesAfterEnv": ["<rootDir>/jest-setup.js"],
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/$1"
  },
  "transform": {
    "^.+\\.(ts|tsx)$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(react-native|@react-native|@expo|expo|expo-.*)/)"
  ]
}